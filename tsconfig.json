{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": "src",
    "paths": {
      "@components/*": ["components/*"],
      "@layouts/*": ["layouts/*"],
      "@actions/*": ["actions/*"],
      "@lib/*": ["lib/*"],
      "@styles/*": ["styles/*"]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}
